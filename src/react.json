{
  "ts-node": {
    "transpileOnly": true,
    "preferTsExts": true
  },
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "outDir": "../../dist/ts",
    "newLine": "lf",
    "declaration": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "nodenext",
    "target": "es2022",
    "lib": [
      "esnext",
      "dom",
      "webworker"
    ],
    "jsx": "react-jsx"
  }
}